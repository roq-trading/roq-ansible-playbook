--name={{ config.name }}
--listen={{ root }}/var/tmp/roq-redis.sock
--metrics={{ root }}/var/tmp/roq-redis_metrics.sock
{% for (key, value) in config.cpu_affinity.items() %}
--{{ key }}-affinity={{ value }}
{% endfor %}
{% for (key, value) in config.redis.items() %}
--redis-{{ key }}={{ value }}
{% endfor %}
{% for (key, value) in config.options.items() %}
--{{ key }}={{ value }}
{% endfor %}
