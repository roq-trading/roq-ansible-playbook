<?xml version="1.0"?>
<yandex>
  <logger>
    <level replace="replace">information</level>
    <console replace="replace">1</console>
  </logger>
  <listen_host replace="replace">0.0.0.0</listen_host>
  <keep_alive_timeout replace="replace">60</keep_alive_timeout>
  <max_table_size_to_drop>0</max_table_size_to_drop>
  <prometheus>
    <endpoint>/metrics</endpoint>
    <port>8001</port>
    <metrics>true</metrics>
    <events>true</events>
    <asynchronous_metrics>true</asynchronous_metrics>
  </prometheus>
</yandex>
